{
  "name": "@rudeland/ui",
  "version": "0.10.6",
  "author": "Rude Ayelo <me@rude.works> (http://rude.works)",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/rudeland-ui.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rudeayelo/rude-ui.git"
  },
  "bugs": {
    "url": "https://github.com/rudeayelo/rude-ui/issues"
  },
  "homepage": "https://github.com/rudeayelo/rude-ui#readme",
  "scripts": {
    "start": "start-storybook -p 9001",
    "build": "build-storybook -c .storybook -o public",
    "build:components": "tsdx build",
    "test": "tsdx test --env=jsdom",
    "lint": "tsdx lint",
    "problems": "tsc --noEmit | grep '.ts' | cut -f1 -d'(' | uniq | xargs code",
    "prerelease": "git checkout master && git pull --tags origin master",
    "release": "standard-version -a",
    "postrelease": "run-s release:*",
    "release:tags": "git push --follow-tags origin master",
    "release:github": "export $(cat .env | xargs) && conventional-github-releaser -p angular",
    "prepublishOnly": "run-s build:components"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tsdx lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "trailingComma": "all"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@storybook/addon-docs": "5.3.17",
    "@storybook/addon-knobs": "5.3.17",
    "@storybook/preset-typescript": "2.1.0",
    "@storybook/react": "5.3.17",
    "@storybook/theming": "5.3.17",
    "@testing-library/react": "10.0.1",
    "@types/color": "3.0.1",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.3",
    "@types/react": "16.9.25",
    "@types/react-dom": "16.9.5",
    "@types/storybook__react": "5.2.1",
    "@types/styled-components": "5.0.1",
    "@types/styled-system": "5.1.9",
    "babel-loader": "8.1.0",
    "commitizen": "4.0.3",
    "conventional-github-releaser": "3.1.3",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "npm-run-all": "4.1.5",
    "react": "16.13.1",
    "react-docgen-typescript-loader": "3.7.1",
    "react-dom": "16.13.1",
    "react-is": "16.13.1",
    "standard-version": "7.1.0",
    "styled-components": "5.0.1",
    "styled-map": "3.3.0",
    "styled-system": "5.1.5",
    "ts-loader": "6.2.2",
    "tsdx": "0.13.0",
    "tslib": "1.11.1",
    "typescript": "3.8.3"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "styled-components": ">=4",
    "styled-system": ">=5"
  },
  "dependencies": {
    "color": "3.1.2",
    "deepmerge": "4.2.2",
    "styled-map": "3.3.0"
  }
}
